{
  "functions": {
    "source": ".",
    "runtime": "nodejs18"
  },
  "hosting": {
    "public": "afya_tracker_web/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}